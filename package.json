{
  "name": "youversion-suggest",
  "title": "YouVersion Bible Suggest",
  "description": "An API for searching the YouVersion Bible and fetching Bible content",
  "version": "3.0.0",
  "license": "MIT",
  "author": "Caleb Evans",
  "homepage": "https://github.com/caleb531/youversion-suggest-api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caleb531/youversion-suggest-api.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "rollup -c",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "tsc --noEmit; eslint 'src/**/*.ts' 'tests/**/*.ts'"
  },
  "dependencies": {
    "@worker-tools/html-rewriter": "0.1.0-pre.19",
    "html-entities": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-wasm": "^6.2.2",
    "@types/node": "^22.15.23",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "glob": "^11.0.2",
    "globals": "^16.2.0",
    "msw": "^2.8.5",
    "prettier": "^3.5.3",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vitest": "^3.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msw"
    ]
  }
}
