{
  "name": "youversion-suggest",
  "title": "YouVersion Bible Suggest",
  "description": "An API for searching the YouVersion Bible and fetching Bible content",
  "version": "1.3.0",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prepare": "rollup -c",
    "test": "pnpm -s build && mocha --require source-map-support/register --require tsx --recursive 'tests/**/*.spec.ts'",
    "coverage": "c8 pnpm -s test",
    "lint": "tsc --noEmit; eslint 'src/**/*.ts' 'tests/**/*.ts'"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/chai": "^4.3.4",
    "@types/cheerio": "^0.22.31",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "mocha": "^10.2.0",
    "nock": "^13.3.1",
    "prettier": "^2.8.8",
    "rollup": "^3.21.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.5.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  }
}
